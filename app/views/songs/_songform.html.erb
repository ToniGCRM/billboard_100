<div class = 'form'>
  <%= form_for @song do |f| %>

    <%= f.label :artist_id %>
    <%= f.text_field :artist_id %>

    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :genre %>
    <%= f.text_field :genre %>

    <%= f.label :rank %>
    <%= f.text_field :rank %>

    <%= f.submit %>
  <% end %>

</div>