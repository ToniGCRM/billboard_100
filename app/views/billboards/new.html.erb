<h1>New Billboard List</h1>
<h3> Add a new billboard list! </h3>

<div class = 'form'>
  <%= form_for @billboard do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :genre %>
    <%= f.text_field :genre %>

    <%= f.label :number %>
    <%= f.text_field :number %>

    <%= f.submit %>
  <% end %>

</div>
